<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include
        android:id="@+id/include1"
        layout="@layout/inews_top" />

    <ImageView
        android:id="@+id/sign_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/sign_one"
        android:layout_centerHorizontal="true"
        android:src="@drawable/ic_sign_two1" />

    <ImageView
        android:id="@+id/sign_five"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sign_two"
        android:layout_alignTop="@+id/sign_frou"
        android:src="@drawable/ic_sign_five1" />

    <ImageView
        android:id="@+id/sign_six"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sign_three"
        android:layout_alignTop="@+id/sign_five"
        android:src="@drawable/ic_sign_six1" />

    <ImageView
        android:id="@+id/sign_three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/sign_frou"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@+id/sign_two"
        android:src="@drawable/ic_sign_three1" />

    <ImageView
        android:id="@+id/sign_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/include1"
        android:layout_marginRight="30dp"
        android:layout_marginTop="30dp"
        android:layout_toLeftOf="@+id/sign_two"
        android:src="@drawable/ic_sign_one1" />

    <ImageView
        android:id="@+id/sign_frou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sign_one"
        android:layout_below="@+id/sign_one"
        android:layout_marginTop="30dp"
        android:src="@drawable/ic_sign_frou1" />

    <ImageButton
        android:id="@+id/sign_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sign_five"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:background="@color/white"
        android:src="@drawable/ic_sign_btn" />

"

</RelativeLayout>