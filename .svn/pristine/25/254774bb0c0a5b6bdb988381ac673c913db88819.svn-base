package com.haier.bean;

import java.util.Collections;
import java.util.List;

public class QuestionInfo {
	private int id;
	private String name;
	private List<QuestionInfo> secondInfos = Collections.emptyList();
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public List<QuestionInfo> getSecondInfos() {
		return secondInfos;
	}
	public void setSecondInfos(List<QuestionInfo> secondInfos) {
		this.secondInfos = secondInfos;
	}
}
