<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@color/transparent" android:state_enabled="true" android:state_window_focused="false"/>
    <item android:drawable="@color/haier_bg_back_pressed" android:state_pressed="true"/>
    <item android:drawable="@color/haier_bg_back_pressed" android:state_enabled="true" android:state_focused="true"/>
    <item android:drawable="@color/transparent" android:state_enabled="true"/>
    <item android:drawable="@color/haier_bg_back_pressed" android:state_focused="true"/>

    <!--
    android:state_selected是选中  
    android:state_focused是获得焦点  
    android:state_pressed是点击  
    android:state_enabled是设置是否响应事件,指所有事件  
    -->

</selector>