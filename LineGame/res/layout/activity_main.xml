<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/layout_full"
    android:id="@+id/relative"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" 
    android:background="@drawable/background">

    <com.example.linegame.LineView
        android:id="@+id/lineview"
        style="@style/layout_full"
        android:layout_above="@+id/btnok" />

    <ImageView
        android:id="@+id/btnok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/subitmit" />

    <TextView
        android:id="@+id/t1"
        android:ems="6"
         android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="false"
        android:layout_alignLeft="@+id/lineview"
        android:layout_alignTop="@+id/lineview"
        android:textColor="#000000"
        android:text="TextView" />

    <TextView
        android:id="@+id/t3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lineview"
        android:ems="6"
        android:textColor="#000000"
         android:singleLine="false"
        android:layout_below="@+id/t2"
        android:layout_marginTop="10dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/t4"
         android:layout_alignLeft="@+id/lineview"
        android:layout_below="@+id/t3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
       
        android:ems="6"
         android:singleLine="false"
        android:layout_marginTop="10dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/lineview"
        android:textColor="#000000"
        android:ems="7"
         android:singleLine="false"
        android:layout_alignTop="@+id/lineview"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="7"
        android:textColor="#000000"
         android:singleLine="false"
        android:layout_alignBottom="@+id/t3"
        android:layout_alignRight="@+id/lineview"
        android:layout_marginRight="0dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/t4"
        android:textColor="#000000"
        android:ems="7"
         android:singleLine="false"
        android:layout_alignRight="@+id/lineview"
        android:layout_marginRight="0dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_alignBaseline="@+id/t2"
        android:layout_alignBottom="@+id/t2"
        android:layout_alignRight="@+id/lineview"
        android:ems="7"
        android:singleLine="false"
        android:text="TextView" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lineview"
        android:layout_below="@+id/t1"
        android:ems="6"
        android:singleLine="false"
        android:text="TextView"
         android:layout_marginTop="10dp"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/t5"
    android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
       android:visibility="invisible"
        android:ems="6"
         android:singleLine="false"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/lineview"
        android:layout_below="@+id/t4"
        android:text="TextView" />

    <TextView
        android:id="@+id/t6"
       android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
       android:visibility="invisible"
        android:ems="6"
         android:singleLine="false"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/lineview"
        android:layout_below="@+id/t5"
        android:text="TextView" />

    <TextView
        android:id="@+id/t7"
         android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
       android:visibility="invisible"
        android:ems="6"
         android:singleLine="false"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/lineview"
        android:layout_below="@+id/t6"
        android:text="TextView" />

    <TextView
        android:id="@+id/t8"
         android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
       android:visibility="invisible"
        android:ems="6"
         android:singleLine="false"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/lineview"
        android:layout_below="@+id/t7"
        android:text="TextView" />

    <TextView
        android:id="@+id/t9"
       android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
       android:visibility="invisible"
        android:ems="6"
         android:singleLine="false"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/lineview"
        android:layout_below="@+id/t8"
        android:text="TextView" />

    <TextView
        android:id="@+id/t10"
         android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
       android:visibility="invisible"
        android:ems="6"
         android:singleLine="false"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@+id/lineview"
        android:layout_below="@+id/t9"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/t5"
        android:layout_alignBottom="@+id/t5"
         android:textColor="#000000"
        android:ems="7"
        android:visibility="invisible"
         android:singleLine="false"
        android:layout_alignRight="@+id/lineview"
        android:layout_marginRight="0dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/t6"
        android:layout_alignBottom="@+id/t6"
         android:textColor="#000000"
        android:ems="7"
        android:visibility="invisible"
         android:singleLine="false"
        android:layout_alignRight="@+id/lineview"
        android:layout_marginRight="0dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/t7"
        android:layout_alignBottom="@+id/t7"
         android:textColor="#000000"
        android:ems="7"
        android:visibility="invisible"
         android:singleLine="false"
        android:layout_alignRight="@+id/lineview"
        android:layout_marginRight="0dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/t8"
        android:layout_alignBottom="@+id/t8"
         android:textColor="#000000"
        android:ems="7"
        android:visibility="invisible"
         android:singleLine="false"
        android:layout_alignRight="@+id/lineview"
        android:layout_marginRight="0dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/t9"
        android:layout_alignBottom="@+id/t9"
        android:textColor="#000000"
        android:ems="7"
        android:visibility="invisible"
         android:singleLine="false"
        android:layout_alignRight="@+id/lineview"
        android:layout_marginRight="0dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/rt10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/t10"
        android:layout_alignBottom="@+id/t10"
         android:textColor="#000000"
        android:ems="7"
        android:visibility="invisible"
         android:singleLine="false"
        android:layout_alignRight="@+id/lineview"
        android:layout_marginRight="0dp"
        android:text="TextView" />

</RelativeLayout>
